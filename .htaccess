IndexIgnore *
Options +FollowSymLinks
RewriteEngine On
#RewriteCond %{REQUEST_URI} !index\.php [NC] # Don't rewrite if we already have
# Include in the next line all folders to exclude
RewriteCond %{REQUEST_URI}  !(licenses|overrides) [NC]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_METHOD} GET
RewriteRule ^(.*)$ cli_process.php [L]

        <Files ~ "0004f.*|sip.cfg|features.cfg">
                ForceType application/xml
        </Files>

	<Files sip.cfg>
		AddType application/xml .cfg
		ForceType application/xml
		DefaultType application/xml
	</Files>

<Files config.json>
    Order allow,deny
    Deny from all
    Allow from 127.0.0.1
</Files>
